{
   "background": {
      "page": "pages/background.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/mousetrap.js", "js/shortkeys.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Easily customizable keyboard shortcuts for Opera",
   "developer": {
      "name": "andreasbovens"
   },
   "homepage_url": "https://github.com/andreasbovens/opera-shortkeys/issues",
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "48": "img/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Shortkeys",
   "options_page": "pages/options.html",
   "permissions": [ "tabs", "clipboardWrite", "clipboardRead", "browsingData", "storage", "bookmarks", "\u003Call_urls>" ],
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "1.6.7"
}
