{
   "author": "gdh1995@qq.com",
   "background": {
      "persistent": true,
      "scripts": [ "background/body.js", "background/commands.js", "background/exclusions.js", "background/tail.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icons/enabled_19.png",
         "38": "icons/enabled_38.png"
      },
      "default_popup": "pages/popup.html",
      "default_title": "Vimium C"
   },
   "chrome_url_overrides": {
      "newtab": "pages/newtab.html"
   },
   "commands": {
      "createTab": {
         "description": "Create new tab"
      },
      "previousTab": {
         "description": "Go one tab left"
      },
      "quickNext": {
         "description": "Go one tab right"
      },
      "reloadTab": {
         "description": "Reload tab"
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content/body.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "front/file_urls.css" ],
      "matches": [ "file:///*" ]
   } ],
   "content_security_policy": "script-src 'self'; style-src 'self' 'unsafe-inline'; font-src 'none'; object-src 'none'; connect-src 'self' data:",
   "description": "customized Vimium in C-Style code, supporting Chinese, global commands and injection into some other extensions",
   "homepage_url": "https://github.com/gdh1995/vimium-c",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "incognito": "spanning",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "35",
   "name": "Vimium C",
   "omnibox": {
      "keyword": "v"
   },
   "options_page": "pages/options.html",
   "permissions": [ "bookmarks", "clipboardRead", "clipboardWrite", "contentSettings", "history", "notifications", "sessions", "storage", "tabs", "webNavigation", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.69.2",
   "web_accessible_resources": [ "content/*", "front/vomnibar.*", "lib/*" ]
}
